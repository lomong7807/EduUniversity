<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="standardhompage.admin.company.mapper.CompanyMapper">
    <select id="getCompanyList" parameterType="companyReq" resultType="companyRes">
        SELECT
            *
        FROM
            edt_company
        ORDER BY
            company_name
        ASC
    </select>
</mapper>